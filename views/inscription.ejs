    <!-- Page Header -->
    <header class="intro-header" style="background-image: url('/images/eau.jpg')">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                    <div class="page-heading" >
                        <h1 >Pas encore membre ? inscrivez-vous</h1>
                        <hr class="small">
                        <span class="subheading" style="background-color: white"><a href="/connection">Connectez-vous le cas écheant</a></span>
                    </div>
                </div>
            </div>
            <div class="row form-group" id="zone_saisie_inscription" style="background-color: white; padding-bottom: 10px;">
				<div class="container">
					<form role="form" method="get" action="#">
						<div class="row">
							<div class="col-lg-6">
								<label for="inscription_lastName">Nom</label>
								<input type="text" class="form-control" id="inscription_lastName" placeholder="Nom" />									
							</div>
							<div class="col-lg-6">
								<label for="inscription_firstName">Prenom(s)</label>
								<input type="text" class="form-control" id="inscription_firstName" placeholder="Prenom(s)" />								
							</div>
						</div>
						<div class="row">
							<div class="col-lg-6">
								<label for="inscription_email">Email</label>
								<input type="text" class="form-control" id="inscription_email" placeholder="exemple@gmail.com" />									
							</div>
						</div>
						<div class="row">
							<div class="col-lg-6">
								<label for="inscription_password">Mot de passe</label>
								<input type="text" class="form-control" id="inscription_password" placeholder="Minimum de six caractères,au moins une lettre majuscule et un chiffre" />									
							</div>
							<div class="col-lg-6">
								<label for="inscription_validate_password">Valider le mot de passe</label>
								<input type="text" class="form-control" id="inscription_validate_password" placeholder="Saisir de nouveau le mot de passe" />									
							</div>
						</div>
						<div class="row">
							<div class="col-lg-6 checkbox">
							  <label><input type="checkbox" value="">En tant que fournissseur d'offres</label>
							</div>
						</div><br/>
						<button type="submit" class="btn btn-primary">Valider</button>
					</form>				
				</div>
            </div>
        </div>
    </header>

    <hr>

